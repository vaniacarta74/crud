{
    "name": "vaniacarta74/crud",
    "description": "Applicazione per la gestione delle operazioni CRUD mediante architettura REST",
    "minimum-stability": "stable",
    "type": "library",
    "keywords": ["CRUD", "RESTful"],    
    "license": "MIT",
    "authors": [
        {
            "name": "Vania Carta",
            "email": "vania.carta@me.com"
        }
    ],
    "support": {
        "source": "https://github.com/vaniacarta74/crud",
        "issues": "https://github.com/vaniacarta74/crud/issues"
    },
    "require": {
        "php": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8"
    },
    "autoload": {
        "files": [
            "src/config/config.php"            
        ],
        "psr-4": {
            "vaniacarta74\\Crud\\": "src/" 
